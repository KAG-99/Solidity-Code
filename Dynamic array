pragma solidity ^0.8.0;
// Dynamic array
contract array
{
    uint[] public arr;

    function pushElement(uint item) public
    {
        arr.push(item);
    }
     function length() public view returns(uint)
    {
        return arr.length; // built in function(length)
    }
    function pop() public 
    {
        arr.pop(); // It removes the last element of the array.
    }
   
}
